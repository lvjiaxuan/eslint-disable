{
  "name": "eslint-disable",
  "displayName": "eslint-disable",
  "version": "0.0.1",
  "description": "eslint disable snippets based on config rules",
  "categories": [
    "Other",
    "Snippets"
  ],
  "main": "./dist/extension.cjs",
  "engines": {
    "vscode": "^1.70.0"
  },
  "type": "module",
  "icon": "",
  "publisher": "lvjiaxuan",
  "activationEvents": [
    "onCommand:eslint-disable.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "eslint-disable.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": []
  },
  "scripts": {
    "compile": "tsup src/extension.ts --external=vscode --format=cjs --clean",
    "dev": "nr compile --watch",
    "vscode:prepublish": "nr compile",
    "pretest": "nr compile && nr lint",
    "lint": "eslint . --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@lvjiaxuan/eslint-config": "^0.0.20",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.22.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "license": "SEE LICENSE IN ./LICENSE",
  "eslintConfig": {
    "extends": "@lvjiaxuan"
  }
}
